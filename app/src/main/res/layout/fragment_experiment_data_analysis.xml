<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@drawable/main_menu_bg">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="Quartiles:"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/fragment_experiment_data_analysis_quartilesDrop"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:clickable="true"
                android:focusable="true"
                app:srcCompat="@android:drawable/btn_dropdown"
                tools:srcCompat="@android:drawable/btn_dropdown" />

        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragment_experiment_data_analysis_quartilesTable"
            android:layout_width="match_parent"
            android:layout_height="210dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="Minimum:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_minimum"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_minimum"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:gravity="end"
                android:hapticFeedbackEnabled="false"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="First Quartile:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_q1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_q1"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:gravity="end"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_exp_data_analysis_minimum" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="Third Quartile:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_q3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_q3"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:gravity="end"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_exp_data_analysis_q1" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="Maximum:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_maximum"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="IQR:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_iqr"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/textView100"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:text="Outlier Percentage:"
                android:textSize="21sp"
                app:layout_constraintEnd_toStartOf="@+id/fragment_exp_data_analysis_outlier_percent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_maximum"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:gravity="end"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_exp_data_analysis_q3" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_iqr"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:gravity="end"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_exp_data_analysis_maximum" />

            <TextView
                android:id="@+id/fragment_exp_data_analysis_outlier_percent"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:gravity="end"
                android:textSize="21sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fragment_exp_data_analysis_iqr" />

        </androidx.constraintlayout.widget.ConstraintLayout>

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_weight="0"
                android:text="Median:"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/fragment_experiment_data_analysis_experimentMedianText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:gravity="end"
                android:text="0"
                android:textSize="30sp" />
        </LinearLayout>

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_weight="0"
                android:text="Mean:"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/fragment_experiment_data_analysis_experimentMeanText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:gravity="end"
                android:text="0"
                android:textSize="30sp" />
        </LinearLayout>

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_weight="0"
                android:text="Stdev.:"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/fragment_experiment_data_analysis_experimentStdevText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:gravity="end"
                android:text="0"
                android:textSize="30sp" />
        </LinearLayout>

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:text="Histogram"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/fragment_experiment_data_analysis_histogramDrop"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:clickable="true"
                app:srcCompat="@android:drawable/btn_dropdown"
                tools:srcCompat="@android:drawable/btn_dropdown"
                android:focusable="true" />
        </LinearLayout>

        <com.jjoe64.graphview.GraphView
            android:id="@+id/fragment_experiment_data_analysis_histogramGraph"
            android:layout_width="match_parent"
            android:layout_height="200dp" />

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:text="Trials Over Time"
                android:textSize="30sp" />

            <ImageView
                android:id="@+id/fragment_experiment_data_analysis_plotsDrop"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="30dp"
                android:layout_weight="1"
                android:clickable="true"
                app:srcCompat="@android:drawable/btn_dropdown"
                tools:srcCompat="@android:drawable/btn_dropdown"
                android:focusable="true" />
        </LinearLayout>

        <com.jjoe64.graphview.GraphView
            android:id="@+id/fragment_experiment_data_analysis_plotsGraph"
            android:layout_width="match_parent"
            android:layout_height="300dp" />

<!--        <View-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="1dp"-->
<!--            android:layout_marginTop="10dp"-->
<!--            android:background="#736F6F" />-->

    </LinearLayout>
</ScrollView>